#ifndef _RTEMS_SCORE_CPUOPTS_H
#define _RTEMS_SCORE_CPUOPTS_H

 
/* if RTEMS_DEBUG is enabled */
#cmakedefine RTEMS_DEBUG

/* if driver manager api is supported */
#cmakedefine RTEMS_DRVMGR_STARTUP

/* if multiprocessing is enabled */
#cmakedefine RTEMS_MULTIPROCESSING

/* if networking is enabled */
#cmakedefine RTEMS_NETWORKING @RTEMS_NETWORKING@

/* if using newlib */
#cmakedefine RTEMS_NEWLIB @RTEMS_NEWLIB@

/* PARAVIRT is enabled */
#cmakedefine RTEMS_PARAVIRT @RTEMS_PARAVIRT@

/* if posix api is supported */
#cmakedefine RTEMS_POSIX_API @RTEMS_POSIX_API@

/* if profiling is enabled */
#cmakedefine RTEMS_PROFILING @RTEMS_PROFILING@

/* if SMP is enabled */
#cmakedefine RTEMS_SMP @RTEMS_SMP@

/* RTEMS version string */
#define RTEMS_VERSION "@RTEMS_VERSION@"

/* The size of `blkcnt_t', as computed by sizeof. */
#cmakedefine SIZEOF_BLKCNT_T @SIZEOF_BLKCNT_T@

/* The size of `blksize_t', as computed by sizeof. */
#cmakedefine SIZEOF_BLKSIZE_T @SIZEOF_BLKSIZE_T@

/* The size of `mode_t', as computed by sizeof. */
#cmakedefine SIZEOF_MODE_T @SIZEOF_MODE_T@

/* The size of `off_t', as computed by sizeof. */
#cmakedefine SIZEOF_OFF_T @SIZEOF_OFF_T@

/* The size of `size_t', as computed by sizeof. */
#cmakedefine SIZEOF_SIZE_T @SIZEOF_SIZE_T@

/* The size of `time_t', as computed by sizeof. */
#cmakedefine SIZEOF_TIME_T @SIZEOF_TIME_T@


/* Define to 1 if ada/gnat bindings are built-in */
#cmakedefine __RTEMS_ADA__ @__RTEMS_ADA__@

/* major version portion of an RTEMS release */
#define __RTEMS_MAJOR__ @__RTEMS_MAJOR__@

/* minor version portion of an RTEMS release */
#define __RTEMS_MINOR__ @__RTEMS_MINOR__@

/* revision version portion of an RTEMS release */
#define __RTEMS_REVISION__ @__RTEMS_REVISION__@

/* sizeof(blkcnt_t) */
#cmakedefine __RTEMS_SIZEOF_BLKCNT_T__ @__RTEMS_SIZEOF_BLKCNT_T__@

/* sizeof(blksize_t) */
#cmakedefine __RTEMS_SIZEOF_BLKSIZE_T__ @__RTEMS_SIZEOF_BLKSIZE_T__@

/* sizeof(mode_t) */
#cmakedefine __RTEMS_SIZEOF_MODE_T__ @__RTEMS_SIZEOF_MODE_T__@

/* sizeof(off_t) */
#cmakedefine __RTEMS_SIZEOF_OFF_T__ @__RTEMS_SIZEOF_OFF_T__@

/* sizeof(time_t) */
#cmakedefine __RTEMS_SIZEOF_TIME_T__ @__RTEMS_SIZEOF_TIME_T__@

#endif /* _RTEMS_SCORE_CPUOPTS_H */
